{
  "name": "open_meta",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "child_process": "^1.0.2",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "dangerously-set-inner-html": "^2.0.1",
    "debug": "^3.1.0",
    "faker": "^4.1.0",
    "form-serialize": "^0.7.2",
    "fs": "0.0.1-security",
    "immutability-helper": "^2.6.4",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.20.1",
    "mongoose": "^5.0.2",
    "morgan": "^1.9.0",
    "morgan-toolkit": "^1.2.2",
    "nodemon": "^1.14.10",
    "pg": "^7.4.1",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-popup": "^2.2.14",
    "semantic-ui-react": "^0.77.2",
    "sequelize": "^4.32.2",
    "sequelize-cli": "^3.2.0",
    "util": "^0.10.3",
    "yargs": "^10.1.1"
  },
  "scripts": {
    "start":
      "concurrently \"react-scripts start\" \"nodemon api/server/index.js\"",
    "update": "npm i && cd api && npm i && cd ..",
    "app": "react-scripts start",
    "api": "nodemon api/server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy-front":
      "npm run build && cd build && mv index.html 200.html && echo openmeta.surge.sh > CNAME && surge .",
    "deploy-back": "git subtree push --prefix api heroku master",
    "deploy-all": "npm run deploy-back && npm run deploy-front",
    "sql:migrate:undo": "cd api && npm run sql db:migrate:undo:all && cd ..",
    "sql:migrate": "cd api && npm run sql db:migrate && cd ..",
    "sql:seed:undo": "cd api && npm run sql db:seed:undo:all && cd ..",
    "sql:seed": "cd api && node ./seeds/sequelize/config.js && cd ..",
    "sql:s":
      "cd api && npm run sql:migrate:undo && npm run sql:migrate && npm run sql:seed && cd ..",
    "mg:seed": "node api/seeds/mongoose",
    "console": "cd api && node repl.js",
    "c": "cd api && node repl.js",
    "sql": "cd api && ./node_modules/sequelize-cli/lib/sequelize"
  },
  "devDependencies": {
    "chalk": "^2.3.0",
    "path": "^0.12.7",
    "react-dev-utils": "^4.2.1"
  }
}
